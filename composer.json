{
  "name": "gurinder/laravel-storage",
  "description": "Laravel Storage",
  "keywords": [
    "laravel",
    "Blog",
    "Taxonomy"
  ],
  "homepage": "https://github.com/gurindersingh/laravel-storage",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gurinder Chauhan",
      "email": "gurinderchauhan@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/container": "^5.6",
    "illuminate/contracts": "^5.6",
    "illuminate/routing": "^5.6",
    "illuminate/support": "^5.6.0",
    "illuminate/validation": "^5.6",
    "illuminate/cache": "^5.6",
    "intervention/image": "^2.4",
    "spatie/temporary-directory": "^1.1",
    "superbalist/flysystem-google-storage": "^7.0",
    "spatie/laravel-image-optimizer": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Gurinder\\Storage\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Gurinder\\Storage\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gurinder\\Storage\\StorageServiceProvider"
      ]
    }
  }
}
